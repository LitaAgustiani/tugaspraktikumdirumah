#include <iostream>
using namespace std;

int main () {
	// menampilkan bilangan ganjil dan genap dari 100-1000
	
	int i = 100;
	while (i <= 1000) {
		if (i % 2 == 1) {
			cout << i << " adalah bilangan ganjil" <<endl;
			i++;
		} else  {
			cout << i << " adalah bilangan genap" <<endl;
			i++;
		}
	}
	return 0;
	
	}
